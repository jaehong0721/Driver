<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/itemPaddingBottom"
                android:paddingLeft="@dimen/itemLeftPadding"
                android:paddingRight="@dimen/itemRightPadding"
                android:paddingTop="@dimen/itemPaddingTop">

    <TextView
        android:id="@+id/tvTimeStamp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="text"
        android:textSize="@dimen/text_large_accent"/>

    <TextView
        android:id="@+id/tvRestaurantName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvTimeStamp"
        android:layout_marginTop="14dp"
        android:text="text"
        android:textSize="@dimen/text_large"/>

    <TextView
        android:id="@+id/tvItems"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvRestaurantName"
        android:layout_marginTop="12dp"
        android:text="text"
        android:textColor="@color/textBlackSub"
        android:textSize="@dimen/text_smallest"/>

    <TextView
        android:id="@+id/tvDeliveryFinish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:text="납품\n완료"
        android:textColor="@color/textBlackSub"
        android:textSize="@dimen/text_large_accent"
        android:visibility="gone"/>
</RelativeLayout>